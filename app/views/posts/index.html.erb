<div class="main posts-index">
  <div class="container">
    <% @posts.each do |post| %>
      <div class="posts-index-item">
        <strong><%= link_to post.user.name, post.user %></strong>
        <%= link_to(post.content, "/posts/#{post.id}") %>
      </div>
    <% end %>
    <%= will_paginate @posts %>
  </div>
</div>
